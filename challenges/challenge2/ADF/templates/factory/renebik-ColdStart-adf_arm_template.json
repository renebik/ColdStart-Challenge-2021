{
    "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "factoryName": {
            "type": "string",
            "metadata": "Data Factory name",
            "defaultValue": "renebik-ColdStart-adf"
        },
        "dataFactory_location": {
            "type": "string",
            "defaultValue": "westeurope"
        },
        "dataFactory_identity_type": {
            "type": "string",
            "defaultValue": "SystemAssigned"
        }
    },
    "variables": {
        "factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
    },
    "resources": [
        {
            "name": "[parameters('factoryName')]",
            "type": "Microsoft.DataFactory/factories",
            "apiVersion": "2018-06-01",
            "properties": {},
            "dependsOn": [],
            "location": "[parameters('dataFactory_location')]",
            "identity": {
                "type": "[parameters('dataFactory_identity_type')]",
                "principalId": "e24e3ba0-6c37-4c9c-a886-bdbb0d5af8fd",
                "tenantId": "7517bc42-bcf8-4916-a677-b5753051f846"
            }
        }
    ]
}